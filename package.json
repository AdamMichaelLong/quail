    "q": "^1.1.2",
    "rainbowvis.js": "^1.0.1",
    "selenium-standalone": "2.43.1-5",
    "stdio": "^0.2.7",
    "webdriverio": "2.3.0"
  },
  "devDependencies": {
    "@quailjs/quail-core": "0.0.15",
    "babel-eslint": "^5.0.0-beta4",
    "chai": "^1.9.2",
    "chai-as-promised": "^4.1.1",
    "chai-spies": "^0.5.1",
