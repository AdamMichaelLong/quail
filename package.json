{
  "name": "quail",
  "description": "Accessibility testing in the browser and on the server.",
  "homepage": "http://quailjs.org",
  "version": "2.2.18",
  "engines": {
    "node": ">= 0.10.20"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/quailjs/quail.git"
  },
  "main": "./quail.js",
  "bin": {
    "quail": "./bin/quail"
  },
  "scripts": {
    "jscs": "jscs -c .jscsrc src/**/*.js test/assessmentSpecs/testRunner.js test/assessmentSpecs/*.js",
    "eslint": "eslint -c .eslintrc src/**/*.js test/assessmentSpecs/*.js",
    "browserify": "node ./scripts/browserifyLib.js && node ./scripts/browserifyDev.js",
    "compile": "npm run jscs && npm run eslint && npm run browserify",
    "prepublish": "npm run compile",
    "test": "bower install && npm run compile && karma start && node ./test/assessmentSpecs/testRunner.js"
  },
  "dependencies": {
    "chai-spies": "^0.5.1",
    "commander": "^2.3.0",
    "deepmerge": "^0.2.7",
    "http-server": "0.7.3",
    "jquery": ">1.4.2",
    "node-fs-extra": "^0.8.1",
    "opener": "^1.4.0",
    "phantomjs": "^1.9.18",
    "q": "^1.1.2",
    "selenium-standalone": "2.43.1-5",
    "stdio": "^0.2.7",
    "webdriverio": "2.3.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "babel-polyfill": "^6.2.0",
    "babelify": "^6.3.0",
    "bower": "^1.3.12",
    "brfs": "^1.4.1",
    "browserify": "^11.0.1",
    "browserify-shim": "^3.8.11",
    "chai": "^1.9.2",
    "chai-as-promised": "^4.1.1",
    "eslint": "^1.3.1",
    "glob": "^4.2.1",
    "grunt": "~0.4.5",
    "grunt-bower-task": "~0.4.0",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-copy": "^0.6.0",
    "grunt-contrib-jshint": "~0.4.3",
    "grunt-contrib-uglify": "~0.2.0",
    "grunt-contrib-watch": "~0.5.3",
    "grunt-convert": "~0.1.5",
    "grunt-exec": "^0.4.6",
    "grunt-execute": "^0.2.2",
    "grunt-gh-pages": "~0.8.1",
    "grunt-run": "^0.4.0",
    "jscs": "jscs-dev/node-jscs#c5adeba",
    "karma": "^0.12.24",
    "karma-browserify": "^4.4.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.1.5",
    "karma-firefox-launcher": "^0.1.3",
    "karma-html-reporter": "^0.2.4",
    "karma-ie-launcher": "^0.1.5",
    "karma-mocha": "^0.1.9",
    "karma-opera-launcher": "^0.1.0",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-safari-launcher": "^0.1.1",
    "karma-sinon": "^1.0.3",
    "karma-spec-reporter": "0.0.13",
    "mkdirp": "^0.5.1",
    "mocha": "^2.0.1",
    "should": "^4.3.0",
    "sinon": "^1.10.3"
  },
  "browserify-shim": {
    "external": "global:External"
  }
}
